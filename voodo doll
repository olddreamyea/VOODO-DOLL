     thingery.MeshType = "Brick"
thingery.Scale = vt(startsize,0.1,startsize)
local dec2 = Instance.new("Decal", lb)
dec2.Face = "Top"
dec2.Texture = "http://www.roblox.com/asset/?id=874580939"
dec2.Color3 = lb.Color
