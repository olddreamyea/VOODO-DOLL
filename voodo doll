		c.Color = C3(0,0,0)
	end]]--
	for _, c in pairs(faceShadingFolder:GetChildren()) do
		c.Color = C3(0,0,0)
	end
	for i, v in pairs(NeonParts) do
		v.Color = BrickColor.Black().Color
	end
