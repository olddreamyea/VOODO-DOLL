	Lightning(Hole.Position,Hole2.p,15.137,3.5,shade,25,1,3,0,true,55) Lightning(Hole.Position,Hole2.p,15.137,3.5,Color3.new(0,0,1),25,1,3,0,true,55)
	for i = 0,2 do
