		if v:IsA("Pose") then
			tbl3[v.Name] = v.CFrame
		end
	end
	return(tbl3)
end

        if playing == true then
			s:Destroy()
			playing = false
			playanother = true
		end
		wait()
		spawn(function()
			if playanother == true then
				playanother = false
			end
			playing = true
