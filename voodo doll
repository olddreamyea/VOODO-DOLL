				CFrame=CF.N(Root.Position)*CF.N(0,-3,0)*CF.A(M.R(90),0,M.R(0+500*M.C(WingSine/360)));
				Color=PrimaryColor;
				Material=Enum.Material.Neon;
				Mesh={Type=Enum.MeshType.Sphere};
