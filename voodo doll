for i = 0,10*2,0.1 do
swait()
scaler2 = scaler2 - 0.1/2
rng.Transparency = rng.Transparency + 0.005
rngm.Scale = rngm.Scale + Vector3.new(scaler2/2.5, scaler2/2.5, scaler2)
end
rng:Destroy()
end))
end

