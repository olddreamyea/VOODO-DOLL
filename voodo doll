			if(Mode=='PHANTASMAGORION'or Mode=='ASCENTED'or Mode=='DESIRABILITY'or Mode=='SYNOPHASIS'or Mode=='EXTINCTION'or Mode=='PANDEMONIUM')then
				Change = .5
