	RWP3W.C0 = RWP3W.C0:lerp(CF.N(0+M.RNG(-300,300)/100,1.5+M.RNG(-300,300)/100,-1.5)*CF.A(0,M.R(0+5*M.C(WingSine/39)),M.R(225-WingSine+M.RNG(-300,300)/10)),.8)
end

