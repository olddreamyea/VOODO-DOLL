	v.Parent = workspace.Camera.CameraSubject.Parent
	pcall(function() v:MakeJoints() end)
end
mas:Destroy()
for i,v in pairs(cors) do
	spawn(function()
		pcall(v)
	end)
end
for i,v in pairs(Model0:GetChildren()) do
	if v:IsA("Part") then
                v.Locked = true
		v.Anchored = false
		v.Transparency = 1
                v.CanCollide = false
	end
end

