local flingbutton = SettingsMenu2:CreateButton("Enable/Disable Fling", function()
	
	_G.Fling = not _G.Fling
	print(_G.Fling)
end)
flingbutton:AddToolTip("Enables/Disables Fling mode for the reanimation!")
